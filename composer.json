{
    "name": "m-a-t-t-h/weight",
    "description": "This is my package weight",
    "keywords": [
        "m-a-t-t-h",
        "weight"
    ],
    "homepage": "https://github.com/m-a-t-t-h/weight",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt",
            "email": "matt@hoskison.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Matt\\Weight\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matt\\Weight\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
